{
  "name": "simple-lit-with-backend-v0.1-simple-lit-with-backend-v0.1",
  "version": "0.4.4",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "npm-run-all --parallel nodemon webpack",
    "nodemon": "npx nodemon src",
    "webpack": "npx webpack -c webpack.dev.cjs --watch",
    "start:test": "npm-run-all --serial webpack:build nodemon",
    "webpack:build": "npx webpack -c webpack.dev.cjs",
    "webpack:build:production": "npx webpack -c webpack.prod.cjs",
    "playwright": "npx playwright install && npx playwright test e2e -c playwright.config.cjs && node e2e/utils/istanbul-lcov.js",
    "semistandard": "semistandard --fix",
    "gcloud:init": "echo \"source ~/google-cloud-sdk/path.bash.inc && source ~/google-cloud-sdk/completion.bash.inc\" >> ~/.bashrc",
    "gcloud:login": "gcloud auth activate-service-account --key-file=config/dev.json",
    "tap": "c8 --lines 80 --functions 80 --branches 80 --statements 80 --report-dir=.nyc_output --reporter=lcov --reporter=text --check-coverage --per-file tap test/**/*.test.js --no-coverage",
    "test": "./node_modules/.bin/npm-run-all --serial semistandard tap playwright",
    "local:get:env": "node node-bash/google/get-local-env.js",
    "ci:get:env": "node node-bash/google/get-env | bash",
    "build": "node node-bash/google/builds-submit | bash",
    "deploy": "node node-bash/google/run-deploy | bash",
    "sentry:release": "node node-bash/sentry/release.js | bash",
    "get-url:cloud-run": "node node-bash/google/get-url | bash",
    "update:traffic:latest": "gcloud run services update-traffic $(jq -r .name package.json)-production --projects $(jq -r .environment.production.google.projectId src/config.json) --to-latest",
    "update": "npx npm-check-updates",
    "postbump": "node postbump",
    "plop": "plop --plopfile plopfile.cjs",
    "create": "npm run plop",
    "create:doc": "npm run plop -- create:doc",
    "create:file": "npm run plop -- create:file",
    "file": "npm run create:file",
    "release": "standard-version",
    "push:tags": "git push --follow-tags origin master",
    "install:docs": "git submodule add --force https://github.com/vjeantet/hugo-theme-docport.git hugo/themes/docport",
    "serve:docs": "hugo serve",
    "serve:docs:gitpod": "hugo server --baseUrl $(gp url 1313) --appendPort=false",
    "create:source": "node jsdoc",
    "update:docs:date": "node hugo-scripts/update-date.js",
    "build:docs": "hugo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tjmonsi/simple-lit-with-backend-v0.1.git"
  },
  "keywords": [
    "base-template"
  ],
  "nyc": {
    "exclude": [
      "test/**/*.test.js"
    ]
  },
  "author": "TJ Monserrat",
  "license": "Apache-2.0",
